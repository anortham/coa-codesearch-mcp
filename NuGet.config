<?xml version="1.0" encoding="utf-8"?>
<!--
  This NuGet.config includes a private Azure DevOps feed for internal development.
  
  External contributors: If you get authentication errors, use NuGet.Public.config instead:
    dotnet restore --configfile NuGet.Public.config
    
  The project uses project references, not NuGet packages, for COA.Mcp.Protocol.
-->
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="COA" value="https://childrensal.pkgs.visualstudio.com/_packaging/COA/nuget/v3/index.json" />
  </packageSources>
  <packageSourceCredentials>
    <COA>
      <add key="Username" value="azure-devops" />
      <add key="ClearTextPassword" value="%COA_NUGET_PAT%" />
    </COA>
  </packageSourceCredentials>
</configuration>