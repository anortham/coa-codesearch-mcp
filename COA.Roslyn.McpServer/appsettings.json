{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "COA.Roslyn": "Debug"
    }
  },
  "McpServer": {
    "MaxWorkspaces": 5,
    "WorkspaceTimeout": "00:30:00",
    "EnableDiagnostics": true,
    "ParallelismDegree": 4,
    "CacheSettings": {
      "MaxCachedWorkspaces": 5,
      "WorkspaceEvictionTime": "00:30:00",
      "EnableSemanticModelCache": true
    }
  }
}